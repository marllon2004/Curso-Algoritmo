programa
{
	/*
	Função: Contar até 10
	Autor: Marllon Araujo
	Aula: 10 Curso em Video
	*/
	
	funcao inicio()
	{
		inteiro contador
		para(contador = 1; contador <= 10; contador++){
			escreva("N° ", contador, "\n")
		}
	}
}
